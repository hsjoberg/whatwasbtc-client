  "dependencies": {
    "bootstrap": "^4.1.1",
    "jquery": "^3.2.1",
    "moment": "^2.22.1",
    "node-sass-chokidar": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.14.3",
    "react": "^16.3.2",
    "react-datepicker": "^1.4.1",
